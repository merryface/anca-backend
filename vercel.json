{
  "version": 2,
  "env": {
      "API_KEY": "API_KEY",
      "API_BASE": "API_BASE"
  },
  "builds": [
      {
          "src": "src/main.ts",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "src/main.ts",
          "methods": [
              "GET"
          ]
      }
  ]
}